{
  "name": "create-app",
  "version": "1.0.0",
  "description": "A boilerplate",
  "main": "server/index.js",
  "scripts": {
    "dev": "cd client && npm start & cd server && npm run dev",
    "start": "cd client && npm start & cd server && npm run dev",
    "build": "webpack --mode=production",
    "lint": "cd eslint .",
    "test": "cd client && npm run test && npm run test:e2e & cd server && npm run test"
  },
  "engines": {
    "node": ">=16.19.1"
  },
  "author": "David Andrea",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.3.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint-plugin-import": "^2.20.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jsmin": "^1.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "module-alias": "^2.2.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "client/*",
      "server/*"
    ]
  },
  "_moduleAliases": {
    "@root": ".",
    "@controllers": "server/controllers",
    "@middleware": "server/middleware",
    "@util": "server/util"
  },
  "packageManager": "yarn@3.5.0"
}
