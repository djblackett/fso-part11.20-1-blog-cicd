{
  "name": "create-app",
  "version": "1.0.0",
  "description": "A boilerplate",
  "scripts": {
    "start:frontend": "webpack-dev-server --open --mode development",
    "start:server:dev": "cd server && node index.js",
    "start:server:prod": "cd server && cross-env NODE_ENV=production node index.js",
    "start:server:test": "cd server && cross-env NODE_ENV=test nodemon index.js",
    "start:fullstack:dev": "wait-on http://localhost:3003 && npm run start:frontend & npx kill-port 3003 && cd server && cross-env NODE_ENV=development nodemon index.js",
    "start:fullstack:prod": "wait-on http://localhost:3003 && npm run start:frontend & npm run start:server:prod",
    "clean": "rm dist/bundle.js",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "deploy": "flyctl deploy",
    "logs:prod": "flyctl logs",
    "eslint": "cd client && eslint . & cd server && eslint .",
    "cy:run": "cd client && cypress run",
    "test:frontend": "cd client && jest",
    "test:backend": "cd server && cross-env NODE_ENV=test jest --verbose --runInBand --forceExit",
    "test": "npm run test:backend & npm run test:frontend",
    "test:e2e": "npx kill-port 3003 && start-server-and-test start:server:test http-get://localhost:3003 cy:run"
  },
  "author": "David Andrea",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.4",
    "@types/jest-expect-message": "^1.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/supertest": "^2.0.12",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "chokidar": "^3.3.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "jest-expect-message": "^1.1.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.2",
    "morgan": "^1.10.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "style-loader": "^3.3.2",
    "wait-on": "^7.0.1",
    "web-vitals": "^3.3.0",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "eslint": "^8.28.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-react": "^7.31.11",
    "html-loader": "^4.2.0",
    "jest": "^29.3.1",
    "jest-watch-typeahead": "^2.2.2",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.20",
    "react-test-renderer": "^18.2.0",
    "supertest": "^6.3.1",
    "webpack-dev-server": "^4.13.1"
  },
  "jest": {
    "projects": [
      "src/client/jest.config.js",
      "src/server/jest.config.js"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "client/*",
      "server/*"
    ]
  }
}
